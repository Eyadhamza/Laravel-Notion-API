{
    "name": "eyadhamza/notion-api-wrapper",
    "description": "A Laravel API Wrapper for the restful Api provided by Notion.so",
    "keywords": [
        "laravel",
        "notion-api-wrapper"
    ],
    "homepage": "https://github.com/eyadhamza/notion-api-wrapper",
    "license": "MIT",
    "authors": [
        {
            "name": "EyadHamza",
            "email": "EyadHamza000@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Pi\\Notion\\": "src"

        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pi\\Notion\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pi\\Notion\\NotionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
