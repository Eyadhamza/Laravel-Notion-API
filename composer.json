{
    "name": "pi/notion-wrapper",
    "description": "This is my package Notion",
    "keywords": [
        "Pi",
        "laravel",
        "notion-wrapper"
    ],
    "homepage": "https://github.com/pi/notion-wrapper",
    "license": "MIT",
    "authors": [
        {
            "name": "EyadHamza",
            "email": "EyadHamza0@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Pi\\Notion\\": "src",
            "Pi\\Notion\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pi\\Notion\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pi\\Notion\\NotionServiceProvider"
            ],
            "aliases": {
                "Notion": "Pi\\Notion\\NotionFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
